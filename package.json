{
	"name": "chatguessr",
	"productName": "Chatguessr",
	"version": "1.0.9",
	"description": "Chatbot for Geoguessr",
	"author": "MrAmericanMike & T.ZHF",
	"license": "ISC",
	"main": "./dist/cg-main/main.js",
	"dependencies": {
		"@paulrosania/better-sqlite3": "^8.1.0",
		"axios": "^0.23.0",
		"country-iso": "^2.0.5",
		"country-iso-3-to-2": "^1.1.0",
		"datatables.net": "^1.11.3",
		"datatables.net-buttons": "^2.0.1",
		"datatables.net-plugins": "^1.10.24",
		"datatables.net-scroller": "^2.0.5",
		"dotenv": "^8.2.0",
		"electron-store": "^8.0.1",
		"electron-updater": "^4.3.9",
		"jquery": "^3.6.0",
		"jquery-ui-dist": "^1.12.1",
		"match-sorter": "^6.3.1",
		"p-map": "^4.0.0",
		"pkg-up": "^3.1.0",
		"tmi.js": "^1.8.5"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.63",
		"@electron-forge/maker-deb": "^6.0.0-beta.63",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.63",
		"@electron-forge/maker-zip": "^6.0.0-beta.63",
		"@electron-forge/publisher-github": "^6.0.0-beta.63",
		"@parcel/transformer-inline-string": "^2.0.0",
		"@tsconfig/node16": "^1.0.2",
		"@types/datatables.net-buttons": "^1.4.7",
		"@types/datatables.net-scroller": "^1.4.1",
		"@types/google.maps": "^3.45.6",
		"@types/jest": "^27.0.2",
		"@types/jquery": "^3.5.7",
		"@types/jqueryui": "^1.12.16",
		"@types/node": "^12.20.29",
		"@types/tmi.js": "^1.8.0",
		"electron": "^17.0.1",
		"jest": "^27.3.1",
		"parcel": "^2.0.0",
		"typescript": "^4.4.4"
	},
	"scripts": {
		"start": "parcel build --no-optimize && electron-forge start",
		"package": "parcel build && electron-forge package",
		"make": "electron-forge make",
		"test": "jest"
	},
	"config": {
		"forge": {
			"packagerConfig": {
				"asar": true,
				"ignore": "\\.parcel-cache"
			},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "chatguessr",
						"iconUrl": "build/icon.ico"
					}
				},
				{
					"name": "@electron-forge/maker-zip",
					"platforms": [
						"darwin"
					]
				},
				{
					"name": "@electron-forge/maker-deb",
					"config": {}
				}
			],
			"publishers": [
				{
					"name": "@electron-forge/publisher-github",
					"config": {
						"repository": {
							"owner": "tzhf",
							"name": "chatguessr"
						}
					}
				}
			]
		}
	},
	"targets": {
		"cg-main": {
			"context": "electron-main",
			"source": "./src/main.js"
		},
		"cg-preload": {
			"context": "electron-renderer",
			"source": "./src/preload.js",
			"scopeHoist": false
		},
		"cg-renderer": {
			"context": "browser",
			"source": "./src/renderer.js"
		},
		"settings": {
			"context": "electron-renderer",
			"outputFormat": "commonjs",
			"source": "./src/Windows/settings/settings.html",
			"publicUrl": "."
		}
	}
}
